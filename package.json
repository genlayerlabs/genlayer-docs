    "clsx": "^2.1.1",
    "next": "15.0.7",
    "nextjs-google-analytics": "^2.3.3",
    "nextra": "^2.13.2",
    "nextra-theme-docs": "^2.13.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
